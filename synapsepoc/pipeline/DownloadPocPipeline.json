{
	"name": "DownloadPocPipeline",
	"properties": {
		"activities": [
			{
				"name": "DownloadPoc",
				"type": "SynapseNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebook": {
						"referenceName": "DownloadPoc",
						"type": "NotebookReference"
					},
					"parameters": {
						"TenantId": {
							"value": {
								"value": "@pipeline().parameters.TenantId",
								"type": "Expression"
							}
						},
						"EnvironmentId": {
							"value": {
								"value": "@pipeline().parameters.EnvironmentId",
								"type": "Expression"
							}
						},
						"SessionId": {
							"value": {
								"value": "@pipeline().parameters.SessionId",
								"type": "Expression"
							}
						}
					},
					"snapshot": true,
					"sparkPool": {
						"referenceName": "ws1sparkpool1",
						"type": "BigDataPoolReference"
					},
					"executorSize": "Small",
					"conf": {
						"spark.dynamicAllocation.enabled": true
					},
					"driverSize": "Small"
				}
			}
		],
		"parameters": {
			"TenantId": {
				"type": "string",
				"defaultValue": "tid-1"
			},
			"EnvironmentId": {
				"type": "string",
				"defaultValue": "eid-1"
			},
			"SessionId": {
				"type": "string",
				"defaultValue": "debug-1"
			}
		},
		"annotations": []
	}
}